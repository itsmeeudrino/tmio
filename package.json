	"scripts": {
		"lint": "biome lint",
		"format": "biome format --write",
		"build": "tsup"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@types/node": "^24.2.0",
		"tsup": "^8.5.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@sinclair/typebox": "^0.34.38"
